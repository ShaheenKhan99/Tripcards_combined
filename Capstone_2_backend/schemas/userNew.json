{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [
      "username",
      "first_name",
      "last_name",
      "email",
      "password"
  ],
  "properties": {
      "username": {
          "title": "The username Schema",
          "type": "string",
          "default": "",
          "examples": [
              "testuser"
          ]
      },
      "first_name": {
          "title": "The first_name Schema",
          "type": "string",
          "default": "",
          "examples": [
              "Test"
          ]
      },
      "last_name": {
          "title": "The last_name Schema",
          "type": "string",
          "default": "",
          "examples": [
              "User"
          ]
      },
      "email": {
          "title": "The email Schema",
          "type": "string",
          "default": "",
          "examples": [
              "testuser@email.com"
          ]
      },
      "password": {
          "title": "The password Schema",
          "type": "string",
          "default": "",
          "examples": [
              "password"
          ]
      },
      "bio": {
          "title": "The bio Schema",
          "type": "string",
          "default": "",
          "examples": [
              "An avid traveler"
          ]
      },
      "is_admin": {
          "title": "The is_admin Schema",
          "type": "boolean",
          "default": false,
          "examples": [
              false
          ]
      }
  },
  "examples": [{
      "username": "testuser",
      "first_name": "Test",
      "last_name": "User",
      "email": "testuser@email.com",
      "password": "password",
      "bio": "An avid traveler",
      "is_admin": "false"
  }]
}