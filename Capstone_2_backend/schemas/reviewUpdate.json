{
  "$schema": "https://json-schema.org/draft-07/schema",
  "$id": "http://example.com/example.json",
  "title": "Root Schema",
  "type": "object",
  "default": {},
  "required": [],
  "properties": {
      "text": {
          "title": "The text Schema",
          "type": "string",
          "default": "",
          "examples": [
              "Went back again to this place since the last time i visited the bay area 5 months ago..."
          ]
      },
      "username": {
          "title": "The username Schema",
          "type": "string",
          "default": "",
          "examples": [
              "harry"
          ]
      },
      "image_url": {
          "title": "The image_url Schema",
          "type": "string",
          "default": "",
          "examples": [
              "https://s3-media3.fl.yelpcdn.com/photo/iwoAD12zkONZxJ94ChAaMg/o.jpg"
          ]
      },
      "rating": {
          "title": "The rating Schema",
          "type": "number",
          "default": 0,
          "examples": [
              4
          ]
      }
  },
  "examples": [{
      "text": "Went back again to this place since the last time i visited the bay area 5 months ago...",
      "username": "harry",
      "rating": 4
  }]
}